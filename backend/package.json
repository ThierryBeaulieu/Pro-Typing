{
  "name": "backend",
  "version": "1.0.0",
  "description": "Golang command line shortcuts",
  "scripts": {
    "docker:run": "docker run --publish 8080:8080 pro-typing-backend:multistage",
    "docker:build": "docker build -t pro-typing-backend:multistage -f Dockerfile.multistage .",
    "docker:tag": "docker tag pro-typing-backend:multistage thierrybeaulieu/pro-typing-backend:v1.0",
    "docker:push": "docker push thierrybeaulieu/pro-typing-backend:v1.0",
    "deploy": "npm run docker:build && npm run docker:tag && npm run docker:push",
    "start": "go run .",
    "test": "./gotest.sh ./... -v",
    "cover": "./gotest.sh ./... -cover",
    "coverage": "./gotest.sh ./... -coverprofile=coverage/coverage.out && go tool cover -html=coverage/coverage.out"
  },
  "author": "",
  "license": "ISC"
}
