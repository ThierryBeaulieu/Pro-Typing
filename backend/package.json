{
  "name": "backend",
  "version": "1.0.0",
  "description": "Golang command line shortcuts",
  "scripts": {
    "docker": "docker run --publish 8080:8080 pro-typing-backend:multistage",
    "build": "docker build -t pro-typing-backend:multistage -f Dockerfile.multistage .",
    "start": "go run .",
    "test": "./gotest.sh ./... -v",
    "cover": "./gotest.sh ./... -cover",
    "coverage": "./gotest.sh ./... -coverprofile=coverage/coverage.out && go tool cover -html=coverage/coverage.out"
  },
